{
  "manifest_version": 2,
  "name": "Dominator",
  "version": "1.0",
  "description": "manipulate dom",
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "devtools",
    "downloads",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "assets/svelte.svg",
      "32": "assets/svelte.svg",
      "48": "assets/svelte.svg",
      "64": "assets/svelte.svg",
      "128": "assets/svelte.svg"
    }
  },
  "devtools_page": "index.html",
  "icons": {
    "16": "assets/svelte.svg",
    "32": "assets/svelte.svg",
    "48": "assets/svelte.svg",
    "64": "assets/svelte.svg",
    "128": "assets/svelte.svg"
  }
}